{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "src/app/api/materiel/route.js", "src/app/api/infos-techniques/route.js", "src/app/api/infos-generales/route.js", "src/app/api/info-techniques-meta/route.js", "src/app/api/etudes/route.js", "src/app/api/etudes/[id]/route.js", "src/app/api/contacts/route.js", "src/app/api/blob-upload/route.js"  ],
  "exclude": [
    "node_modules"
  ]
}
